	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;전회 사정했다
		IF TCVAR:102
			IF TALENT:연모 || TALENT:연인
			;前回フランちゃんが絶頂しているかで分岐？作る。あとＶ感度分岐も
				SELECTCASE NOWEX:TotalEX
				CASE 5 TO 15
					PRINTFORML 「흐ー읏%UNICODE(0x2665)%흐ー읏%UNICODE(0x2665)%…… 아앗, 아으응!!」
					PRINTFORML 「앙, 덋%UNICODE(0x2665)% 이미 가써%UNICODE(0x2665)% 가써여%UNICODE(0x2665)%」
					PRINTFORMW 「아앙%UNICODE(0x2665)%뎨에엣, 또옷%UNICODE(0x2665)%가버리니까아%UNICODE(0x2665) *3%」
				CASE 2 TO 4
					PRINTFORML 「앗으응%UNICODE(0x2665)%안대, 가써, 갔는데에%UNICODE(0x2665)%」
					PRINTFORMW 「아지히익, 질내에, 단단햇%UNICODE(0x2665)%」
				CASE 1
					PRINTFORML 「우～～읏! 앗, 햐앙, 가버린 참인데…」
					PRINTFORMW 「앗, 안댓, 아직, 안쪽에, 단단햇, 으응!」
				CASEELSE
					PRINTFORML 「앗, 응, 아직 단단하네에」
					PRINTFORMW 「……한 번 더, 할래? 아, 우응…」
				ENDSELECT
			ELSE
			ENDIF
			RETURN 1
		ELSE
		;出さずに継続？
			IF TALENT:연모 || TALENT:연인
				;5超えると普通に超強絶頂するんでここ分岐で
				IF ABL:1 >= 5
					PRINTFORML 「앗%UNICODE(0x2665)%햐앙, 거기%UNICODE(0x2665)%」
					PRINTFORML 「기분 좋아%UNICODE(0x2665)%앗, 가버려어%UNICODE(0x2665)%」
					PRINTFORMW 「히양%UNICODE(0x2665)% %CALLNAME:MASTER%, %CALLNAME:MASTER%%UNICODE(0x2665) *3%」
				ELSEIF ABL:1 >= 3
					PRINTFORML 「앗, 히얏, %CALLNAME:MASTER%, 거기, 좋아%UNICODE(0x2665)%」
					PRINTFORMW 「앗, 응읏! 기분 좋아, %CALLNAME:MASTER%%UNICODE(0x2665)%」
				ELSE
					PRINTFORML 「응읏, 기분좋아, %CALLNAME:MASTER%」
					PRINTFORMW 「그래, 거기, 안쪽에, 빙글빙글해서, 좋아, %CALLNAME:MASTER%……!」
				ENDIF
			ELSE
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;기본세트
	;시간(時姦)중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;本日最初のSEX
	ELSEIF TCVAR:50:351 == 0
		IF TALENT:연인 || TALENT:연모
		;TCVAR:351は恋人OR恋慕でしか変動しないため不要？
			IF ABL:1 >= 5
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「저, 최근에, %조사처리(CALLNAME:MASTER, "가")% 해줄 때 왠지 금방 가버려서……」
					PRINTFORMW 「그러니까……상냥하게 했음 좋겠다～ 라던가……앗%UNICODE(0x2665)%」
				CASE 1
					PRINTFORML 「이 자세, %CALLNAME:MASTER%의 얼굴이 보여서 좋더라」
					PRINTFORMW 「아, 그래도, 분명 잔뜩 가버린 내 얼굴도 보이겠네……%UNICODE(0x2665)%」
				ENDSELECT
			ELSEIF ABL:1 >= 3
				PRINTFORML 「이 자세, %CALLNAME:MASTER%의 얼굴이 보여서 좋더라」
				PRINTFORMW 「사랑받고 있다는, 그런 느낌도 들어. ……잔뜩 이뻐해줘?」
			ELSE
				PRINTFORML 「응읏, 이 자세로, 한다……는 거네」
				PRINTFORML 「그러니까, 그게, 모처럼인데……키스, 했으면 좋겠어」
				PRINTFORMW 「응, 츄%UNICODE(0x2665)%…… 자아, 하자」
			ENDIF
		ELSE
		ENDIF
		TCVAR:50:351 += 1
		RETURN 1
	;괘씸3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

